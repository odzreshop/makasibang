                        <h4>Free to Use</h4>
                        <p>This API is completely free to use for all your project needs.</p>
                    </div>
                </div>
            </div>

        </main>
        
        <main id="api-view" style="display: none;">
             <header id="api-header">
                <img src="https://nefuu.my.id/zWfi.png" alt="API Logo" class="profile-pic">
                <div class="header-text">
                    <h1 id="current-category-title"></h1>
                    <p>Dynamic REST API Service</p>
                </div>
            </header>
            <% for (const categoryName in categories) { %>
                <% categories[categoryName].forEach(endpoint => { %>
                    <div class="endpoint-container" data-category="<%= categoryName %>" style="display: none;">
                        <div class="endpoint-header">
                            <div class="endpoint-info">
                                <span class="method-badge"><%= endpoint.method || 'GET' %></span>
                                <span class="endpoint-path">/api/<%= endpoint.category.toLowerCase() %>/<%= endpoint.name.toLowerCase().replace(/ /g, '-') %></span>
                            </div>
                             <svg class="chevron-icon" viewBox="0 0 24 24"><path d="M7.41 8.59L12 13.17l4.59-4.58L18 10l-6 6-6-6 1.41-1.41z"></path></svg>
                        </div>
                        <div class="interaction-panel">
                            <p class="endpoint-description"><%= endpoint.description %></p>
                            <% if (endpoint.parameters && endpoint.parameters.length > 0) { %>
                                <% endpoint.parameters.forEach(param => { %>
                                    <% if (param.type === 'file') { %>
                                        <div class="panel-section file-input-wrapper">
                                            <label><%= param.name %> <% if (param.required) { %><span>*</span><% } %></label>
                                            <input type="file" id="param-<%= endpoint.name %>-<%= param.name %>" data-param-name="<%= param.name %>" class="file-input" accept="image/*">
                                            <label for="param-<%= endpoint.name %>-<%= param.name %>" class="file-input-label">
                                                <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"><path d="M9 16h6v-6h4l-7-7-7 7h4v6zm-4 2h14v2H5v-2z"></path></svg>
                                                <span>Choose Image</span>
                                            </label>
                                            <span class="file-name-display">No file chosen</span>
                                        </div>
                                    <% } else { %>
                                        <div class="panel-section">
                                            <label for="param-<%= endpoint.name %>-<%= param.name %>"><%= param.name %> <% if (param.required) { %><span>*</span><% } %></label>
                                            <input type="text" id="param-<%= endpoint.name %>-<%= param.name %>" data-param-name="<%= param.name %>" placeholder="<%= param.description %>">
                                        </div>
                                    <% } %>
                                <% }) %>
                            <% } %>
                            <div class="button-group">
                                <button class="panel-btn execute-btn">
                                    <svg viewBox="0 0 24 24"><path d="M8 5v14l11-7z"></path></svg>
                                    Execute
                                </button>
                                <button class="panel-btn visit-btn">
                                    <svg viewBox="0 0 24 24"><path d="M10 6v2h2.59L4 16.59 5.41 18 14 9.41V12h2V6h-6z"></path></svg>
                                    Visit
                                </button>
                            </div>
                            <div class="result-container">
                                <label class="result-label">Request URL:</label>
                                <div class="request-url-container">
                                    <pre class="request-url"></pre>
                                    <button class="copy-btn">
                                        <svg class="icon-copy" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"><path d="M16 1H4c-1.1 0-2 .9-2 2v14h2V3h12V1zm3 4H8c-1.1 0-2 .9-2 2v14c0 1.1.9 2 2 2h11c1.1 0 2-.9 2-2V7c0-1.1-.9-2-2-2zm0 16H8V7h11v14z"></path></svg>
                                        <svg class="icon-check" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"><path d="M9 16.17L4.83 12l-1.42 1.41L9 19 21 7l-1.41-1.41z"></path></svg>
                                    </button>
                                </div>
                                <div class="result-header">
                                    <label class="result-label">Result:</label>
                                    <div class="status-code"></div>
                                </div>
                                <div class="result-wrapper">
                                    <div class="result-pre">
                                        <pre><code class="language-json"></code></pre>
                                        <div class="loading-wave">
                                          <div class="loading-bar"></div><div class="loading-bar"></div><div class="loading-bar"></div><div class="loading-bar"></div>
                                        </div>
                                    </div>
                                    <button class="copy-btn">
                                        <svg class="icon-copy" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"><path d="M16 1H4c-1.1 0-2 .9-2 2v14h2V3h12V1zm3 4H8c-1.1 0-2 .9-2 2v14c0 1.1.9 2 2 2h11c1.1 0 2-.9 2-2V7c0-1.1-.9-2-2-2zm0 16H8V7h11v14z"></path></svg>
                                        <svg class="icon-check" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"><path d="M9 16.17L4.83 12l-1.42 1.41L9 19 21 7l-1.41-1.41z"></path></svg>
                                    </button>
                                </div>
                                <ul class="response-codes-legend">
                                    <li class="status-2xx"><svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"><path d="M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm-2 15l-5-5 1.41-1.41L10 14.17l7.59-7.59L19 8l-9 9z"></path></svg><b>200</b> OK - Request successful</li>
                                    <li class="status-4xx"><svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"><path d="M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm1 15h-2v-2h2v2zm0-4h-2V7h2v6z"></path></svg><b>400</b> Bad Request - Invalid parameters</li>
                                    <li class="status-warn"><svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"><path d="M1 21h22L12 2 1 21zm12-3h-2v-2h2v2zm0-4h-2V8h2v6z"></path></svg><b>429</b> Too Many Requests</li>
                                    <li class="status-5xx"><svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"><path d="M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm1 15h-2v-2h2v2zm0-4h-2V7h2v6z"></path></svg><b>500</b> Internal Server Error</li>
                                </ul>
